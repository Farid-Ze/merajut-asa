{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "buildCommand": "turbo run build --filter=@merajut-asa/web",
  "outputDirectory": "apps/web/.next",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": [
    "sin1"
  ],
  "functions": {
    "apps/web/src/app/**": {
      "maxDuration": 30
    }
  }
}